    "def fun():\n",
    "    x=input(\"Введите число:\")\n",
    "    s=str(x)\n",
    "    lst=[]\n",
    "    sum=0\n",
    "    for i in s:\n",
    "        lst.append(i)\n",
    "    for k in lst:\n",
    "        if int(k)%2==1:\n",
    "            sum=sum+(int(k))**2\n",
    "    print(sum)"
